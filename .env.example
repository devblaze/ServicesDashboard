# Database Configuration
DB_USER=admin
DB_PASSWORD=your_secure_password_here
DB_PORT=5432

# Backend Configuration
# Port exposed to the host machine
BACKEND_PORT=5050
# Internal port inside the container (usually keep at 8080)
BACKEND_INTERNAL_PORT=8080

# Frontend Configuration
# Port exposed to the host machine
FRONTEND_PORT=5173
# Internal port inside the container (usually keep at 80)
FRONTEND_INTERNAL_PORT=80

# Ollama AI Integration (Optional)
OLLAMA_URL=http://host.docker.internal:11434

# Environment
ASPNETCORE_ENVIRONMENT=Production
